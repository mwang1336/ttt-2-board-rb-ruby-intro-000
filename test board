board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
def display_board(board)
  board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
  puts display(board)
  puts "   |   |   "
  puts "-----------"
  puts "   |   |   "
  puts "-----------"
  puts "   |   |   "
  puts "A Tic Tac Toe Board"
end


